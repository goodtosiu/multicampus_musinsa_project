    "# 1ï¸âƒ£ url â†’ brand slug (lcdctm) ì¶”ì¶œ í•¨ìˆ˜\n",
    "# ==================================================\n",
    "def extract_brand_slug(url):\n",
    "    \"\"\"\n",
    "    https://www.musinsa.com/brand/lcdctm\n",
    "    https://www.musinsa.com/brand/lcdctm/\n",
    "    â†’ lcdctm\n",
    "    \"\"\"\n",
    "    if not isinstance(url, str):\n",
    "        return None\n",
    "    try:\n",
    "        after_brand = url.split(\"/brand/\")[1]\n",
    "        slug = after_brand.split(\"/\")[0]\n",
    "        return slug\n",
    "    except IndexError:\n",
    "        return None\n",
    "\n",
    "# slug ì»¬ëŸ¼ ìƒì„±\n",
    "df_de_null[\"brand_slug\"] = df_de_null[\"url\"].apply(extract_brand_slug)\n",
    "\n",
    "# ==================================================\n",
    "# 2ï¸âƒ£ 403 ë°©ì§€ìš© í—¤ë”\n",
    "# ==================================================\n",
    "HEADERS = {\n",
